{
  "name": "@angular-node-electron/source",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Angular + Node + Electron demo",
  "author": "ganatan",
  "scripts": {
    "start:frontend": "nx serve frontend-angular",
    "lint:frontend": "nx lint frontend-angular",
    "test:frontend": "nx test frontend-angular",
    "build:frontend": "nx build frontend-angular --base-href ./",
    "e2e:frontend": "nx e2e frontend-angular-e2e",
    "start:backend": "nx serve backend-typescript",
    "lint:backend": "nx lint backend-typescript",
    "test:backend": "nx test backend-typescript",
    "build:backend": "nx build backend-typescript",
    "e2e:backend": "nx e2e backend-typescript-e2e",
    "dev:electron": "electron apps/electron/src/main.js",
    "start:electron": "electron apps/electron/src/main.js",
    "lint:electron": "nx lint electron",
    "test:electron": "nx test electron",
    "build:electron": "electron-builder",
    "e2e:electron": "nx e2e electron-e2e",
    "generate:structure": "ts-node tools/scripts/generate-structure.ts"
  },
  "build": {
    "appId": "com.ganatan.demo",
    "productName": "GanatanElectronApp",
    "asar": false,
    "directories": {
      "output": "dist/electron"
    },
    "files": [
      "apps/electron/**/*",
      "dist/apps/frontend-angular/browser/**/*",
      "package.json",
      "!**/node_modules/**",
      "node_modules/ws/**/*",
      "node_modules/dotenv/**/*"
    ],
    "extraMetadata": {
      "main": "apps/electron/src/main.js"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/common": "~20.3.0",
    "@angular/compiler": "~20.3.0",
    "@angular/core": "~20.3.0",
    "@angular/forms": "~20.3.0",
    "@angular/platform-browser": "~20.3.0",
    "@angular/platform-browser-dynamic": "~20.3.0",
    "@angular/router": "~20.3.0",
    "@fortawesome/fontawesome-free": "6.7.2",
    "axios": "^1.6.0",
    "bootstrap": "5.3.7",
    "dotenv": "17.0.1",
    "express": "^4.21.2",
    "rxjs": "~7.8.0",
    "ws": "^8.18.3",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/core": "~20.3.0",
    "@angular-devkit/schematics": "~20.3.0",
    "@angular/build": "~20.3.0",
    "@angular/cli": "~20.3.0",
    "@angular/compiler-cli": "~20.3.0",
    "@angular/language-service": "~20.3.0",
    "@eslint/js": "^9.8.0",
    "@nx/angular": "22.0.4",
    "@nx/devkit": "22.0.4",
    "@nx/esbuild": "22.0.4",
    "@nx/eslint": "22.0.4",
    "@nx/eslint-plugin": "22.0.4",
    "@nx/jest": "22.0.4",
    "@nx/js": "22.0.4",
    "@nx/node": "^22.0.4",
    "@nx/playwright": "22.0.4",
    "@nx/web": "22.0.4",
    "@nx/workspace": "22.0.4",
    "@playwright/test": "^1.36.0",
    "@schematics/angular": "~20.3.0",
    "@swc-node/register": "~1.9.1",
    "@swc/core": "~1.5.7",
    "@swc/helpers": "~0.5.11",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/node": "20.19.9",
    "@typescript-eslint/utils": "^8.40.0",
    "angular-eslint": "^20.3.0",
    "electron": "^39.2.2",
    "electron-builder": "^26.0.12",
    "esbuild": "^0.19.2",
    "eslint": "^9.8.0",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-playwright": "^1.6.2",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-preset-angular": "~14.6.1",
    "jest-util": "^29.7.0",
    "nx": "22.0.4",
    "prettier": "^2.6.2",
    "ts-jest": "^29.1.0",
    "ts-node": "10.9.1",
    "tslib": "^2.3.0",
    "typescript": "~5.9.2",
    "typescript-eslint": "^8.40.0"
  }
}