{
  "name": "inventory-contract",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "shared/inventory-contract/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/shared/inventory-contract",
        "tsConfig": "shared/inventory-contract/tsconfig.lib.json",
        "packageJson": "shared/inventory-contract/package.json",
        "main": "shared/inventory-contract/src/index.ts",
        "assets": ["shared/inventory-contract/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "shared/inventory-contract/jest.config.ts"
      }
    }
  }
}
