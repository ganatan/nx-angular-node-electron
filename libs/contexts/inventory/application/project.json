{
  "name": "inventory-application",
  "$schema": "../../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/contexts/inventory/application/src",
  "projectType": "library",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nx/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/contexts/inventory/application",
        "tsConfig": "libs/contexts/inventory/application/tsconfig.lib.json",
        "packageJson": "libs/contexts/inventory/application/package.json",
        "main": "libs/contexts/inventory/application/src/index.ts",
        "assets": ["libs/contexts/inventory/application/*.md"]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/contexts/inventory/application/jest.config.ts"
      }
    }
  }
}
